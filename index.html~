	<section id="biens">
		<script>
			function drawBiens(){
				for(var i in biens ) {
					biens[i].attachToMap( map );
					map.entities.push( biens[i].bubble );
					
					var input = biens[i].listView();

					$("#biens").append( input );
					biens[i].bubble.init();
					
					if( ns.length < 10 )
						ns.push(biens[i].bubble);
				}

			 	Microsoft.Maps.Events.removeHandler( initevent );
				Microsoft.Maps.Events.addHandler( map, 'viewchangeend', function() { ns.temperature = 3; /* ns.attractionRepulsion();*/ } );
			 	// Hack to force offset after pushing entity. Workaround for Bing Maps Bug.
				map.setView({ center: new Microsoft.Maps.Location( 47.2185047, -1.5446583 ) });
				
				
				ns.ratataPlop();
				 /* for( var i = 0; i < 10; i++ ) {

					biens[i].bubble.applyForce( new Vector( -30, 50) );
					biens[i].bubble.plop();
				} */

			}
			</script>
	</section>

